<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="i.farmer.demo.recyclerview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/i_farmer"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.RecyclerView">
        <activity android:name=".DemoExample">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".tab.TabViewActivity" android:label="RecyclerTabView"/>
    </application>

</manifest>